<config-template xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
                 <device tags="merge">
                 <name>{/name}</name>
                 <address>{/mgmt_address}</address>
                 <port>{/port}</port>
                 <authgroup>{/authgroup}</authgroup>
                 <device-type>
                   <netconf>
                     <ned-id xmlns:cumulus-nclu-nc-1.0="http://tail-f.com/ns/ned-id/cumulus-nclu-nc-1.0">cumulus-nclu-nc-1.0:cumulus-nclu-nc-1.0</ned-id>
                   </netconf>
                 </device-type>
                 <state>
                   <admin-state>unlocked</admin-state>
                 </state>
               </device>
    </devices>
</config-template>